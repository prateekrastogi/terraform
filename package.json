{
    "name": "terraform",
    "version": "1.0.0",
    "keywords": [
        "loopback-application",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "build:apidocs": "lb-apidocs",
        "dev": "cross-env NODE_ENV=dev nodemon --ext 'ts' --delay 2 .",
        "lint": "tslint *.ts --fix && lb-prettier \"**/*.ts\" \"**/*.js\" --write",
        "migrate": "node ./dist/migrate",
        "start": "tsc && cross-env NODE_ENV=prod node .",
        "start:graphql": "oasgraph http://localhost:3000/openapi.json",
        "watch": "tsc --watch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "npm run lint"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/prateekrastogi/dripfeed"
    },
    "license": "Apache-2.0",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist",
        "src"
    ],
    "dependencies": {
        "@loopback/boot": "^1.2.1",
        "@loopback/context": "^1.11.0",
        "@loopback/core": "^1.4.1",
        "@loopback/openapi-v3": "^1.3.5",
        "@loopback/repository": "^1.4.1",
        "@loopback/rest": "^1.10.2",
        "@loopback/rest-explorer": "^1.1.16",
        "applicationinsights": "^1.3.1",
        "cross-env": "^5.2.0",
        "cuid": "^2.1.6",
        "faker": "^4.1.0",
        "isomorphic-unfetch": "^3.0.0",
        "loopback-connector-mongodb": "^4.1.0",
        "oasgraph-cli": "^0.4.1",
        "public-ip": "^3.0.0",
        "puppeteer": "^1.14.0",
        "rxjs": "^6.4.0"
    },
    "devDependencies": {
        "@loopback/build": "^1.4.3",
        "@loopback/testlab": "^1.2.4",
        "@loopback/tslint-config": "^2.0.4",
        "@types/puppeteer": "^1.12.3",
        "husky": "^1.3.1",
        "nodemon": "^1.18.11",
        "tslint": "^5.16.0",
        "typescript": "^3.4.4"
    }
}
